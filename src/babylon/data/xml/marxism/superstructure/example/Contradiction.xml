<?xml version="1.0" encoding="UTF-8"?>
<Contradictions>
  <!-- Principal Contradiction Between Proletariat and Bourgeoisie -->
  <Contradiction>
    <!-- Basic Information -->
    <ID>CON001</ID>
    <Name>Class Struggle</Name>
    <Description>The fundamental contradiction between the Proletariat and the Bourgeoisie over the ownership of the means of production.</Description>

    <!-- Parties Involved -->
    <BetweenEntities>
      <Entity>
        <EntityID>C001</EntityID> <!-- Proletariat -->
        <EntityType>Class</EntityType>
        <Role>Oppressed</Role>
      </Entity>
      <Entity>
        <EntityID>C002</EntityID> <!-- Bourgeoisie -->
        <EntityType>Class</EntityType>
        <Role>Oppressor</Role>
      </Entity>
    </BetweenEntities>

    <!-- Nature of Contradiction -->
    <Universality>true</Universality>
    <Particularity>The struggle over exploitation and surplus value.</Particularity>
    <PrincipalContradiction>true</PrincipalContradiction>
    <PrincipalAspect>C002</PrincipalAspect> <!-- Bourgeoisie as the dominant class -->
    <SecondaryAspect>C001</SecondaryAspect>

    <!-- Attributes -->
    <Antagonism>Antagonistic</Antagonism>
    <Intensity>High</Intensity>
    <State>Active</State>

    <!-- Transformation -->
    <PotentialForTransformation>true</PotentialForTransformation>
    <ConditionsForTransformation>
      <Condition>The Proletariat gains class consciousness.</Condition>
      <Condition>Economic crisis intensifies exploitation.</Condition>
    </ConditionsForTransformation>

    <!-- Resolution -->
    <ResolutionMethods>
      <Method>Revolution</Method>
      <Method>Reform</Method>
    </ResolutionMethods>
    <ResolutionConditions>
      <Condition>Overthrow of the Bourgeoisie.</Condition>
      <Condition>Establishment of collective ownership.</Condition>
    </ResolutionConditions>

    <!-- Effects -->
    <Effects>
      <Effect>
        <Target>SocialClasses</Target>
        <Attribute>ClassConsciousness</Attribute>
        <ModificationType>Increase</ModificationType>
        <Value>0.15</Value> <!-- Increases by 15% -->
        <Description>Heightens class awareness among the Proletariat.</Description>
      </Effect>
      <Effect>
        <Target>EconomicSystem</Target>
        <Attribute>Stability</Attribute>
        <ModificationType>Decrease</ModificationType>
        <Value>0.10</Value> <!-- Decreases by 10% -->
        <Description>Leads to economic instability due to unrest.</Description>
      </Effect>
    </Effects>

    <!-- Attributes -->
    <Attributes>
      <Attribute>
        <Name>HistoricalSignificance</Name>
        <Value>Primary</Value>
      </Attribute>
    </Attributes>
  </Contradiction>

  <!-- Secondary Contradiction Between Factions -->
  <Contradiction>
    <!-- Basic Information -->
    <ID>CON002</ID>
    <Name>Ideological Dispute</Name>
    <Description>Disagreement between the Revolutionary Front and the Reformist Alliance over strategies for social change.</Description>

    <!-- Parties Involved -->
    <BetweenEntities>
      <Entity>
        <EntityID>F003</EntityID> <!-- Revolutionary Front -->
        <EntityType>Faction</EntityType>
        <Role>Radical</Role>
      </Entity>
      <Entity>
        <EntityID>F007</EntityID> <!-- Reformist Alliance -->
        <EntityType>Faction</EntityType>
        <Role>Moderate</Role>
      </Entity>
    </BetweenEntities>

    <!-- Nature of Contradiction -->
    <Universality>false</Universality>
    <Particularity>Methods of achieving social change.</Particularity>
    <PrincipalContradiction>false</PrincipalContradiction>
    <PrincipalAspect>F003</PrincipalAspect> <!-- Revolutionary Front -->
    <SecondaryAspect>F007</SecondaryAspect>

    <!-- Attributes -->
    <Antagonism>Non-Antagonistic</Antagonism>
    <Intensity>Medium</Intensity>
    <State>Active</State>

    <!-- Transformation -->
    <PotentialForTransformation>true</PotentialForTransformation>
    <ConditionsForTransformation>
      <Condition>External suppression intensifies.</Condition>
      <Condition>Common enemy emerges.</Condition>
    </ConditionsForTransformation>

    <!-- Resolution -->
    <ResolutionMethods>
      <Method>Negotiation</Method>
      <Method>Coalition Building</Method>
    </ResolutionMethods>
    <ResolutionConditions>
      <Condition>Agreement on a unified strategy.</Condition>
    </ResolutionConditions>

    <!-- Effects -->
    <Effects>
      <Effect>
        <Target>Factions</Target>
        <Attribute>Unity</Attribute>
        <ModificationType>Increase</ModificationType>
        <Value>0.20</Value> <!-- Increases unity by 20% -->
        <Description>Potential to strengthen cooperation between factions.</Description>
      </Effect>
    </Effects>

    <!-- Attributes -->
    <Attributes>
      <Attribute>
        <Name>StrategicImportance</Name>
        <Value>Secondary</Value>
      </Attribute>
    </Attributes>
  </Contradiction>

</Contradictions>