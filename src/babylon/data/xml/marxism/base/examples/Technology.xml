<?xml version="1.0" encoding="UTF-8"?>
<Technologies>
  <!-- Advanced Metallurgy -->
  <Technology>
    <!-- Basic Information -->
    <ID>T001</ID>
    <Name>Advanced Metallurgy</Name>
    <Category>Economic</Category>
    <Description>Improves the efficiency of metal production units.</Description>
    
    <!-- Prerequisites -->
    <PrerequisiteTechnologies>
      <TechnologyID>T005</TechnologyID> <!-- Basic Metallurgy -->
    </PrerequisiteTechnologies>
    <RequiredResources>
      <ResourceRequirement>
        <ResourceID>R007</ResourceID> <!-- Research Data -->
        <Quantity>100</Quantity>
      </ResourceRequirement>
    </RequiredResources>
    
    <!-- Research Details -->
    <ResearchCost>500</ResearchCost>
    <ResearchTime>10</ResearchTime>
    
    <!-- Effects -->
    <Effects>
      <Effect>
        <Target>ProductionUnit</Target>
        <Attribute>Efficiency</Attribute>
        <ModificationType>Increase</ModificationType>
        <Value>0.10</Value> <!-- Increases efficiency by 10% -->
        <Description>Boosts efficiency of metal-related production units.</Description>
      </Effect>
    </Effects>
    
    <!-- Availability -->
    <AvailableToFactions>
      <FactionID>F001</FactionID> <!-- Ruling Class FactionID -->
      <FactionID>F002</FactionID> <!-- Corporate Elites FactionID -->
    </AvailableToFactions>
    
    <!-- Attributes -->
    <Attributes>
      <Attribute>
        <Name>ResearchArea</Name>
        <Value>Materials Science</Value>
      </Attribute>
    </Attributes>
  </Technology>
  
  <!-- Sustainable Farming Techniques -->
  <Technology>
    <!-- Basic Information -->
    <ID>T002</ID>
    <Name>Sustainable Farming Techniques</Name>
    <Category>Economic</Category>
    <Description>Enhances agricultural productivity while preserving resources.</Description>
    
    <!-- Prerequisites -->
    <PrerequisiteTechnologies>
      <TechnologyID>T006</TechnologyID> <!-- Agricultural Practices -->
    </PrerequisiteTechnologies>
    <RequiredResources>
      <ResourceRequirement>
        <ResourceID>R008</ResourceID> <!-- Scientific Journals -->
        <Quantity>50</Quantity>
      </ResourceRequirement>
    </RequiredResources>
    
    <!-- Research Details -->
    <ResearchCost>300</ResearchCost>
    <ResearchTime>7</ResearchTime>
    
    <!-- Effects -->
    <Effects>
      <Effect>
        <Target>ProductionUnit</Target>
        <Attribute>OutputQuantity</Attribute>
        <ModificationType>Increase</ModificationType>
        <Value>0.15</Value> <!-- Increases output by 15% -->
        <Description>Improves the output of farms and reduces environmental impact.</Description>
      </Effect>
    </Effects>
    
    <!-- Availability -->
    <AvailableToClasses>
      <ClassID>C003</ClassID> <!-- Petit Bourgeoisie -->
      <ClassID>C004</ClassID> <!-- Peasantry -->
    </AvailableToClasses>
    
    <!-- Attributes -->
    <Attributes>
      <Attribute>
        <Name>EnvironmentalImpact</Name>
        <Value>Low</Value>
      </Attribute>
    </Attributes>
  </Technology>
  
  <!-- Military Encryption Systems -->
  <Technology>
    <!-- Basic Information -->
    <ID>T003</ID>
    <Name>Military Encryption Systems</Name>
    <Category>Military</Category>
    <Description>Secures military communications against enemy interception.</Description>
    
    <!-- Prerequisites -->
    <PrerequisiteTechnologies>
      <TechnologyID>T007</TechnologyID> <!-- Basic Communications -->
    </PrerequisiteTechnologies>
    <RequiredResources>
      <ResourceRequirement>
        <ResourceID>R009</ResourceID> <!-- Electronic Components -->
        <Quantity>200</Quantity>
      </ResourceRequirement>
    </RequiredResources>
    
    <!-- Research Details -->
    <ResearchCost>700</ResearchCost>
    <ResearchTime>12</ResearchTime>
    
    <!-- Effects -->
    <Effects>
      <Effect>
        <Target>MilitaryUnit</Target>
        <Attribute>CommunicationSecurity</Attribute>
        <ModificationType>Enable</ModificationType>
        <Description>Prevents enemy intelligence from intercepting communications.</Description>
      </Effect>
      <Effect>
        <Target>StateIntelligence</Target>
        <Attribute>SignalIntelligence</Attribute>
        <ModificationType>Decrease</ModificationType>
        <Value>0.20</Value> <!-- Reduces enemy's signal intelligence effectiveness by 20% -->
        <Description>Reduces the effectiveness of enemy signal intelligence.</Description>
      </Effect>
    </Effects>
    
    <!-- Availability -->
    <AvailableToFactions>
      <FactionID>F001</FactionID> <!-- Ruling Class FactionID -->
    </AvailableToFactions>
    
    <!-- Attributes -->
    <Attributes>
      <Attribute>
        <Name>SecurityLevel</Name>
        <Value>High</Value>
      </Attribute>
    </Attributes>
  </Technology>
  
</Technologies>