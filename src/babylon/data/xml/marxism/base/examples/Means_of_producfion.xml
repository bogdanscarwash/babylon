<?xml version="1.0" encoding="UTF-8"?>
<MeansOfProduction>
  <!-- Steel Factory Alpha -->
  <ProductionUnit>
    <ID>P001</ID>
    <Name>Steel Factory Alpha</Name>
    <Type>Factory</Type>
    <Description>A large steel production facility equipped with modern machinery.</Description>

    <!-- Ownership -->
    <OwnerType>Faction</OwnerType>
    <OwnerID>F001</OwnerID>

    <!-- Location -->
    <LocationID>L003</LocationID>

    <!-- Production Details -->
    <InputResources>
      <ResourceRequirement>
        <ResourceID>R001</ResourceID> <!-- Iron Ore -->
        <Quantity>1000</Quantity>
      </ResourceRequirement>
      <ResourceRequirement>
        <ResourceID>R004</ResourceID> <!-- Coal -->
        <Quantity>500</Quantity>
      </ResourceRequirement>
    </InputResources>
    <OutputResources>
      <ResourceOutput>
        <ResourceID>R002</ResourceID> <!-- Steel -->
        <Quantity>800</Quantity>
      </ResourceOutput>
    </OutputResources>
    <ProductionCapacity>800</ProductionCapacity>
    <Efficiency>0.85</Efficiency>

    <!-- Labor Details -->
    <RequiredLabor>200</RequiredLabor>
    <LaborType>Skilled</LaborType>

    <!-- Technology -->
    <TechnologyLevel>Industrial</TechnologyLevel>
    <RequiredTechnologies>
      <TechnologyID>T002</TechnologyID> <!-- Advanced Metallurgy -->
    </RequiredTechnologies>

    <!-- Maintenance and Upkeep -->
    <MaintenanceCost>5000.0</MaintenanceCost>
    <Condition>Good</Condition>

    <!-- Attributes -->
    <Attributes>
      <Attribute>
        <Name>EmissionLevel</Name>
        <Value>High</Value>
      </Attribute>
    </Attributes>
  </ProductionUnit>

